<view class="common-list u-f animated fadeInLeft fast"><view class="common-list-l"><image src="{{item.userPic}}" mode="widthFix" lazy-load></image></view><view class="common-list-r"><view class="u-f-ac u-f-jsb"><view class="u-f-ac">{{item.userName+''}}<tag-gender-age vue-id="1" gender="{{item.gender}}" age="{{item.age}}" bind:__l="__l"></tag-gender-age></view><block wx:if="{{!isFollow}}"><view data-event-opts="{{[['tap',[['follow']]]]}}" class="icon iconfont icon-zengjia" bindtap="__e">关注</view></block><block wx:else><view data-event-opts="{{[['tap',[['follow']]]]}}" bindtap="__e">已关注</view></block></view><view>{{item.title}}</view><view class="u-f-ajc"><block wx:if="{{item.PicTextStyle}}"><image src="{{item.titlePic}}" mode="widthFix" lazy-load></image></block><block wx:if="{{item.videoStyle}}"><image src="{{item.titlePic}}" mode="widthFix" lazy-load></image><view class="common-list-play icon iconfont icon-bofang"></view><view class="common-list-playinfo">{{item.videoStyle.playNum+" 次播放 "+item.videoStyle.length}}</view></block><block wx:if="{{item.shareStyle}}"><view class="common-list-share u-f-ac"><image src="{{item.shareStyle.sharePic}}" mode="widthFix" lazy-load></image><view>{{item.shareStyle.shareTitle}}</view></view></block></view><view class="u-f-ac u-f-jsb"><view>{{item.location}}</view><view class="u-f-ac"><view class="icon iconfont icon-zhuanfa">{{item.shareNum}}</view><view class="icon iconfont icon-pinglun">{{item.commentNum}}</view><view class="icon iconfont icon-dianzan">{{item.likeNum}}</view></view></view></view></view>