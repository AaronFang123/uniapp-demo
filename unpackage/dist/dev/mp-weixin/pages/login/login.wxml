<view><uni-status-bar vue-id="1" backgroundColor="#009687" bind:__l="__l"></uni-status-bar><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="icon iconfont icon-guanbi" bindtap="__e"></view><view class="pureColor"></view><block wx:if="{{isLoginById}}"><view class="body"><input class="uni-input common-input" type="text" placeholder="昵称/手机号/邮箱" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/><view class="login-input-box"><input class="uni-input common-input forget-input" type="password" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['','pw','$event',[]]]]]]}}" value="{{pw}}" bindinput="__e"/><view class="forget u-f-ajc">忘记密码？</view></view></view></block><block wx:else><view class="body"><view class="login-input-box"><view class="phone-verify u-f-ajc">+86</view><input class="uni-input common-input phone-verify-input" type="text" placeholder="手机号码(仅限中国大陆)" data-event-opts="{{[['input',[['__set_model',['','phoneNum','$event',[]]]]]]}}" value="{{phoneNum}}" bindinput="__e"/></view><view class="login-input-box"><input class="uni-input common-input vericode-input" type="text" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['','vericode','$event',[]]]]]]}}" value="{{vericode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getVericode',['$event']]]]]}}" class="{{['vericode u-f-ajc',(vericodeAble)?'vericode-active':'']}}" bindtap="__e">{{''+(!countDown?'获取验证码':countDown+'s')+''}}</view></view></view></block><button class="{{['user-setting-logout-btn ',(disable)?'user-set-btn-disable':'']}}" type="primary" disabled="{{disable}}" loading="{{loading}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">登录</button><block wx:if="{{isLoginById}}"><view data-event-opts="{{[['tap',[['loginSwitch',['$event']]]]]}}" class="login-status u-f-ajc " bindtap="__e">验证码登陆<view class="icon iconfont icon-jinru"></view></view></block><block wx:else><view data-event-opts="{{[['tap',[['loginSwitch',['$event']]]]]}}" class="login-status u-f-ajc " bindtap="__e">账号密码登陆<view class="icon iconfont icon-jinru"></view></view></block><view class="other-login-title u-f-ajc">第三方登录</view><other-login vue-id="2" bind:__l="__l"></other-login><view class="login-agreement u-f-ajc login-color">注册即代表您同意<view>《xxx》协议</view></view></view>